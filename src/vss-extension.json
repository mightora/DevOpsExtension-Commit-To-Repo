{
    "manifestVersion": 1,
    "id": "mightora-commit-to-repo-extension",
    "name": "Commit to Repo Extension",
    "version": "2.0.8",
    "publisher": "mightoraio",
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This extension provides a single task to commit files to a repository.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/mightora_io_icon_260x260.png"
    },
    "content": {
        "details": {
            "path": "details.md"
        },
        "pricing": {
            "path": "pricing.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://mightora.io"
        },
        "getstarted": {
            "uri": "https://mightora.io"
        },
        "learn": {
            "uri": "https://mightora.io"
        },
        "support": {
            "uri": "https://mightora.io"
        },
        "repository": {
            "uri": "https://github.com/mightora/Power-Platform-DevOps-Extension"
        },
        "issues": {
            "uri": "https://github.com/mightora/Power-Platform-DevOps-Extension/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mightora/Power-Platform-DevOps-Extension"
    },
    "badges": [
        {
            "href": "https://marketplace.visualstudio.com/items?itemName=mightoraio.mightora-power-platform-devOps-extension",
            "uri": "https://img.shields.io/badge/Marketplace-View%20Extension-blue?logo=visual-studio",
            "description": "VS Marketplace"
        }
    ],
    "files": [
        {
            "path": "tasks/commitToRepo",
            "packagePath": "commitToRepo"
        }
    ],
    "contributions": [
        {
            "id": "mightora-commitToRepo-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "commitToRepo"
            }
        }
    ]
}
