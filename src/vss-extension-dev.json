{
    "manifestVersion": 1,
    "id": "mightora-commit-to-repo-extension-dev",
    "name": "DEV - Commit to Repo Extension",
    "version": "1.0.84",
    "publisher": "mightoraio",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "This extension provides a single task to commit files to a repository.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/mightora_io_icon_260x260.png"
    },
    "content": {
        "details": {
            "path": "readme.md"
        },
        "pricing": {
            "path": "pricing.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://mightora.io"
        },
        "getstarted": {
            "uri": "https://mightora.io"
        },
        "learn": {
            "uri": "https://mightora.io"
        },
        "support": {
            "uri": "https://mightora.io"
        },
        "repository": {
            "uri": "https://mightora.io"
        },
        "issues": {
            "uri": "https://mightora.io"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/mightora"
    },
    "files": [
        {
            "path": "src/commitToRepo",
            "packagePath": "commitToRepo"
        }
    ],
    "contributions": [
        {
            "id": "mightora-commitToRepo-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "commitToRepo"
            }
        }
    ]
}